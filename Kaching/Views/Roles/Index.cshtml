@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>  
@{  
    ViewData["Title"] = "Index";  
}  
  
<h1>List of Roles</h1>  
<a asp-action="Create">Create</a>  

<table class="ui celled unstackable table">
    <thead>
    <tr><th>Id</th>
        <th>Name</th>
    </tr></thead>
    <tbody>
     @foreach (var role in Model)
     {
        <tr>  
            <td>@role.Id</td>  
            <td>@role.Name</td>  
        </tr>  
    }
    </tbody>
</table>